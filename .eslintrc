{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": "off",
    "no-constant-condition": "off",
    "no-useless-escape": "off",
    "no-prototype-builtins": "off"
  }
}
